{
	"Angular Module": {
		"prefix": "ngmodule",
		"description": "Angular 1.x Module",
		"body": [
			"(function () {",
			"    'use strict';",
			"",
			"    angular.module('${module}', [",
			"        // Angular modules",
			"",
			"        // Custom modules",
			"",
			"        // 3rd Party Modules",
			"",
			"    ]);",
			"})();"
		]
	},
	"Angular Controller using $scope": {
		"prefix": "ngcontrollerscope",
		"description": "Angular 1.x Controller using $scope",
		"body": [
			"(function(){",
			"    'use strict';",
			"",
			"    angular",
			"        .module('${module}')",
			"        .controller('${controller}', ${controller})",
			"",
			"    ${controller}.$$inject = ['$$scope'];",
			"",
			"    function ${controller}($$scope) {",
			"",
			"        activate();",
			"",
			"        function activate() { }",
			"    }",
			"})();"
		]
	},
	"Angular Controller": {
		"prefix": "ngcontroller",
		"description": "Angular 1.x Controller",
		"body": [
			"(function(){",
			"    'use strict';",
			"",
			"    angular",
			"        .module('${module}')",
			"        .controller('${controller}', ${controller})",
			"",
			"    ${controller}.$$inject = ['$$location'];",
			"",
			"    function ${controller}($$location) {",
			"        /* jshint validthis:true */",
			"        var vm = this;",
			"",
			"        activate();",
			"",
			"        function activate() { }",
			"    }",
			"})();"
		]
	},
	"Angular Service": {
		"prefix": "ngservice",
		"description": "Angular 1.x Service",
		"body": [
			"(function(){",
			"    'use strict';",
			"",
			"    angular",
			"        .module('${module}')",
			"        .service('${service}', ${service})",
			"",
			"    ${service}.$$inject = ['$$http'];",
			"",
			"    function ${service}($$http) {",
			"        this.getData = getData;",
			"",
			"        function getData() { }",
			"    }",
			"})();"
		]
	},
	"Angular Factory": {
		"prefix": "ngfactory",
		"description": "Angular 1.x Factory",
		"body": [
			"(function(){",
			"    'use strict';",
			"",
			"    angular",
			"        .module('${module}')",
			"        .factory('${factory}', ${factory})",
			"",
			"    ${factory}.$$inject = ['$$http'];",
			"",
			"    function ${factory}($$http) {",
			"        var service = {",
			"            getData: getData",
			"        };",
			"",
			"        return service;",
			"",
			"        function getData() { }",
			"    }",
			"})();"
		]
	},
	"Angular Constant": {
		"prefix": "ngconstant",
		"description": "Angular 1.x Constant",
		"body": [
			"(function(){",
			"    'use strict';",
			"",
			"    angular",
			"        .module('${module}')",
			"        .constant('${constant}', ${constant});",
			"",
			"}());"
		]
	},
	"Angular Directive": {
		"prefix": "ngdirective",
		"description": "Angular 1.x Directive",
		"body": [
			"(function () {",
			"    'use strict';",
			"",
			"    angular",
			"        .module ('${module}')",
			"        .directive ('${directive}', ${directive});",
			"",
			"    ${directive}.$$inject = ['$$window'];",
			"",
			"    function ${directive}($$window) {",
			"        // Usage:",
			"        //     <directive></directive>",
			"        // Creates:",
			"        //",
			"        var directive = {",
			"            link: link,",
			"            restrict: 'EA'",
			"        };",
			"        return directive;",
			"",
			"        function link(scope, element, attrs) {",
			"        }",
			"    }",
			"",
			"})();"
		]
	},
	"Angular Filter": {
		"prefix": "ngfilter",
		"description": "Angular 1.x Filter",
		"body": [
			"(function(){",
			"    'use strict';",
			"",
			"    angular",
			"        .module('${module}')",
			"        .filter('${filter}', ${filter})",
			"",
			"    function ${filter}(){",
			"",
			"        return ${filter}Filter;",
			"",
			"        function ${filter}Filter(input){",
			"",
			"            return input",
			"        }",
			"    }",
			"",
			"}());"
		]
	},
	"Angular Provider": {
		"prefix": "ngprovider",
		"description": "Angular 1.x Provider",
		"body": [
			"(function(){",
			"    'use strict';",
			"",
			"    angular",
			"        .module('${module}')",
			"        .provider('${provider}', ${provider}Provider)",
			"",
			"    function ${provider}Provider() {",
			"        var configValue = false;",
			"        this.setConfigValue = function (value) {",
			"            configValue = value;",
			"        };",
			"",
			"        this.$$get = providerFactory;",
			"",
			"        providerFactory.$$inject = ['$$http'];",
			"        function providerFactory($$http) {",
			"            var service = {",
			"                getData: getData",
			"            };",
			"",
			"            return service;",
			"",
			"",
			"            function getData() {",
			"                return configValue;",
			"            }",
			"        }",
			"    }",
			"})();"
		]
	},
	"Angular Config": {
		"prefix": "ngconfig",
		"description": "Angular 1.x Config",
		"body": [
			"(function(){",
			"    'use strict';",
			"",
			"    angular",
			"        .module('${module}')",
			"        .config(${config}Config)",
			"",
			"    ${config}Config.$$inject = [];",
			"",
			"    function ${config}Config() {",
			"",
			"    }",
			"",
			"}());"
		]
	},
	"Angular Run": {
		"prefix": "ngrun",
		"description": "Angular 1.x Run",
		"body": [
			"(function(){",
			"    'use strict';",
			"",
			"    angular",
			"        .module('${module}')",
			"        .run(${run}Run)",
			"",
			"	${run}Run.$$inject = [];",
			"",
			"	function ${run}Run(){",
			"	}",
			"",
			"}());"
		]
	}
	,
	"Angular Decorator": {
		"prefix": "ngdecorator",
		"description": "Angular 1.x Decorator",
		"body": [
			"(function(){",
			"    'use strict';",
			"",
			"    angular",
			"        .module('${module}')",
			"        .config(${decorator}Decorator)",
			"",
			"	config${decorator}Decorator.$$inject = ['$$provide'];",
			"",
			"	function config${decorator}Decorator($$provide){",
			"		function _${targetService}Decorator($$delegate){",
			"",
			"			function _decoratedTarget(){",
			"				//pre",
			"				return $$delegate.${target}()",
			"				//post",
			"			}",
			"",
			"			return {",
			"				${target}: _decoratedTarget",
			"			}",
			"		}",
			"		$$provide.decorator(${targetService}, _${targetService}Decorator);",
			"	}",
			"",
			"}());"
		]
	}
}